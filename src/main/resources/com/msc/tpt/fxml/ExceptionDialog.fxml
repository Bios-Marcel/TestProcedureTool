<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="titleLable" text="Exception occured" />
      <Label fx:id="headerTextLabel" />
      <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="Infinity" maxWidth="Infinity" VBox.vgrow="ALWAYS">
         <content>
            <TextArea fx:id="stackTraceTextArea" editable="false" maxHeight="Infinity" maxWidth="Infinity" />
         </content>
      </ScrollPane>
      <Label text="%text.inordertohelp.reportbug" wrapText="true" />
      <HBox alignment="CENTER_RIGHT" maxWidth="Infinity" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#reportBug" text="%button.reportbug" />
          <Button mnemonicParsing="false" onAction="#closeDialog" text="%general.button.close" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
